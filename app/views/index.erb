        <header>
            <h1>Finstagram</h1>
        </header>
        
        <main role="main">
            
<article class="post">
    <div class="user-info">
        <img src="<%= @post_TheACT[:avatar_url] %>" alt="<%= @post_TheACT[:username] %>">
        <h2><%= @post_TheACT[:username] %></h2>
        <h3><%= @post_TheACT[:humanized_time_ago] %></h3>
    </div>
    <a class="photo" href="#">
        <img src="<%= @post_TheACT[:photo_url] %>" alt="post from <%= @post_TheACT[:username] %>"
    </a>
    <div class="actions">
        <%= @post_TheACT[:like_count] %> likes
        <span class="comment-count"><%= @post_TheACT[:comment_count] %> comment</span>
    </div>
    <ul class="comments">
        <li>
            <p>
                <%= @post_TheACT[:comments][0][:username] %>: <%= @post_TheACT[:comments][0][:text] %>
            </p>
        </li>
    </ul>
</article>

